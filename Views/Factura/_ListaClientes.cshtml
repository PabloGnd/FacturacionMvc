@model List<Cliente>
@foreach (var item in Model)
{
    <div class="col-1 text-center  ">
                            <h3 class="text-center"><i class="fa-regular fa-circle-user text-primary"></i></h3>
                        </div>

                        <div class="col-8 text-center align-middle" >
                            <h6 class="small">@item.Identificacion - @item.Nombre1 @item.Apellido1</h6>
                        </div>
                        <div class="col-1 text-center"><a id="btnSeleccionarCliente"  class="nav-link px-0 align-middle" data-bs-toggle="modal" data-bs-target="#"><i class="fa-solid fa-magnifying-glass"></i></a></div>
                    
    
    


    <input id="txtNombre1" name="Nombre1" type="hidden" value="@item.Nombre1">
}



  